pipeline {
  agent any
  tools {
  maven "maven"
  }
  stages {
    stage('Build') { // Get code
      steps {
        // get code from our Git repository
        git 'https://github.com/rahmanca/DevOps-Demo-WebApp.git'
        sh "mvn clean compile"
        }
        post{
        success {
          echo "Success"
          }
       }
      } 
    }
  }
